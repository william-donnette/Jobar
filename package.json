{
	"name": "jobar",
	"version": "1.2.14",
	"description": "Jobar is a TypeScript library designed to orchestrate workflows with Temporal and expose them seamlessly via an Express API. It simplifies Temporal integration by managing workflows, activities, and task queues while providing a structured way to handle API endpoints for workflow execution.",
	"keywords": [
		"jobar",
		"temporal",
		"typescript",
		"express",
		"workflow",
		"dashboard",
		"framework",
		"sdk",
		"activity",
		"task",
		"queue",
		"worker"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"test": "nyc mocha --exit --require ts-node/register --require source-map-support/register src/**/*.spec.ts",
		"build": "tsup"
	},
	"author": "William Donnette",
	"license": "MIT",
	"devDependencies": {
		"@temporalio/testing": "^1.11.7",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/mocha": "^10.0.10",
		"eslint": "^9.12.0",
		"mocha": "^11.1.0",
		"nyc": "^17.1.0",
		"ts-node": "^10.9.2",
		"tsup": "^8.1.0",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@temporalio/activity": "^1.11.7",
		"@temporalio/client": "^1.11.7",
		"@temporalio/worker": "^1.11.7",
		"@temporalio/workflow": "^1.11.7",
		"cors": "^2.8.5",
		"express": "^4.21.2",
		"winston": "^3.17.0"
	}
}
